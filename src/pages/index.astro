          entries.forEach((entry) => {
            const square = entry.target.querySelector(
              ".anime_svg"
            ) as HTMLElement;
            if (entry.isIntersecting && square) {
              square.style.animation = "down_to_up 4s linear";
              square.style.opacity = "0.5";
              return;
            }
            // square.classList.remove('square-animation');
          });
        });
        document.querySelectorAll("[class*=anime_svg]").forEach((r) => {
          observer.observe(r);
        });
