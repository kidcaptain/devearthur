<!DOCTYPE html>
<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
    <style>
      html {
        scroll-behavior: smooth;
      }
      p.p-style {
        position: relative;
        overflow: hidden;
      }
      .pass {
        position: relative;
        z-index: 2;
        padding-bottom: 20px;
        /* background-color: white; */
        /* letter-spacing: 0.3ch; */
        transition: cubic-bezier(0.175, 0.885, 0.32, 1.275) 2s;
      }
      .pass-two {
        z-index: 1;
        position: absolute;
        opacity: 0;
        transform: translateX(-100%);
        transition: cubic-bezier(0.175, 0.885, 0.32, 1.275) 2s;
      }
      @keyframes down_to_up {
        0% {
          transform: translate(-4px, 100px);
          opacity: 0;
        }
        100% {
          transform: translateY(-4px, -8px);
          opacity: 1;
        }
      }

      p.p-style:hover .pass-two {
        transform: translateX(0px);
        left: 0;
        opacity: 1;
        transition: cubic-bezier(0.175, 0.885, 0.32, 1.275) 2s;
      }
      p.p-style:hover .pass {
        opacity: 0;
        transition: cubic-bezier(0.175, 0.885, 0.32, 1.275) 2s;
      }
    </style></head
  >

